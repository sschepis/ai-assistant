{
    "compilerOptions": {
        "outDir": "dist",
        "moduleResolution": "node",
        "target": "es6",
        "module": "commonjs",
        "strict": false,
        "baseUrl": ".",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": false,
        "resolveJsonModule": true,
        "typeRoots": ["./node_modules/@types"],
        "downlevelIteration": false,
        "lib": ["ES2021"]

    },
    "exclude": ["node_modules"]
}